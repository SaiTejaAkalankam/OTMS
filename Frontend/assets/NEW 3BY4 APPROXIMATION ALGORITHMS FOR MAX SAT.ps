/landscape 	{/Orientation landscapeMode 	def} bdef

/dpi2point 0 def

/FontSize 0 def
/FMS {
	/FontSize xstore		%save size off stack
	findfont
	[FontSize dpi2point mul 0 0 FontSize dpi2point mul neg 0 0]
	makefont
	def
	}bdef
/setPortrait {
	1 dpi2point div -1 dpi2point div scale
	llx ury neg translate
	} bdef
/setLandscape {
	1 dpi2point div -1 dpi2point div scale
	urx ury neg translate
	90 rotate
	} bdef

/csm {Orientation portraitMode eq {setPortrait} {setLandscape} ifelse} bdef
/SO { [] 0 setdash } bdef
/DO { [.5 dpi2point mul 4 dpi2point mul] 0 setdash } bdef
/DA { [4 dpi2point mul] 0 setdash } bdef
/DD { [.5 dpi2point mul 4 dpi2point mul 4 dpi2point mul 4 dpi2point mul] 0 setdash } bdef

/L {	% LineTo
	lineto
	stroke
	} bdef
/MP {	% MakePoly
	3 1 roll moveto
	1 sub {rlineto} repeat
	} bdef
/AP {	% AddPoly
	{rlineto} repeat
	} bdef
/PP {	% PaintPoly
	closepath fill
	} bdef
/DP {	% DrawPoly
	closepath stroke
	} bdef
/MR {	% MakeRect
	4 -2 roll moveto
	dup  0 exch rlineto
	exch 0 rlineto
	neg  0 exch rlineto
	closepath
	} bdef
/FR {	% FrameRect
	MR stroke
	} bdef
/PR {	% PaintRect
	MR fill
	} bdef
/L1i {	% Level 1 Image
	{ currentfile picstr readhexstring pop } image
	} bdef

/half_width 0 def
/half_height 0 def
/MakeOval {
	newpath
	/ury xstore /urx xstore /lly xstore /llx xstore
	/half_width  urx llx sub 2 div store
	/half_height ury lly sub 2 div store
	llx half_width add lly half_height add translate
	half_width half_height scale
	.5 half_width div setlinewidth
	0 0 1 0 360 arc
	} bdef
/FO {
	gsave
	MakeOval stroke
	grestore
	} bdef
/PO {
	gsave
	MakeOval fill
	grestore
	} bdef

/PD {
	2 copy moveto lineto stroke
	} bdef


